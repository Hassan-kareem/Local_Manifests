<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="PE"
    fetch="https://github.com/PixelExperience-Devices" />
  <remote name="Gitlab"
    fetch="https://gitlab.pixelexperience.org/android/vendor-blobs" />
  <remote name="NRanjan"
    fetch="https://github.com/NRanjan-17" />
  <remote name="Hassan"
    fetch="https://github.com/Hassan-kareem" />

  <remove-project path="frameworks/base/data/fonts" />
  <project path="frameworks/base/data/fonts" name="Fonts" remote="Hassan" revision="main" />
  
  <!-- Device Tree -->
  <project path="device/xiaomi/mido" name="device_xiaomi_mido" remote="PE" revision="thirteen-plus" />
  <!-- Kernel Tree -->
  <project path="kernel/xiaomi/mido" name="kernel_xiaomi_mido" remote="NRanjan" revision="thirteen" />
  <!-- Vendor Tree -->
  <project path="vendor/xiaomi/mido" name="vendor_xiaomi_mido" remote="Gitlab" revision="thirteen" />
</manifest>
